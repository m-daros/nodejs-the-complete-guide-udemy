{
  "name": "lecture495",
  "version": "1.0.0",
  "description": "Exercises for lecture495",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --watch './**/*.ts' --exec 'ts-node' src/ts/app.ts",
    "test": "find ./test/unit -name '*.spec.js' | xargs mocha -R spec",
    "e2e-test": "find ./test/e2e -name '*.spec.js' | xargs mocha -R spec",
    "test-coverage": "find ./test/unit -name '*.spec.js' | xargs nyc mocha -R spec"
  },
