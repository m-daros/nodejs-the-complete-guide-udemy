    "type": "git",
    "url": "git+https://github.com/m-daros/nodejs-the-complete-guide-udemy.git"
  },
  "bugs": {
    "url": "https://github.com/m-daros/nodejs-the-complete-guide-udemy/issues"
  },
  "homepage": "https://github.com/m-daros/nodejs-the-complete-guide-udemy#readme",
  "author": "m-daros",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "socket.io": "^4.1.3",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.6",
    "@types/sequelize": "^4.28.10",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "chai": "^4.3.4",
    "install": "^0.13.0",
    "mocha": "^9.1.0",
    "nodemon": "^2.0.12",
    "npm": "^7.21.1",
